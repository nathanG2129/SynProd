{
  "name": "backend",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "gradlew.bat build",
        "cwd": "backend"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "gradlew.bat bootRun",
        "cwd": "backend"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "gradlew.bat test",
        "cwd": "backend"
      }
    }
  }
}